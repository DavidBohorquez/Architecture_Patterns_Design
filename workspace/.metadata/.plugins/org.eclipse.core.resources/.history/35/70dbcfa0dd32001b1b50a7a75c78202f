package edu.logica;

import edu.cableado.IAplicable;
import edu.cableado.IModerador;

public class TuPerfil implements IModerador {

	@Override
	public void moderar() {
		IAplicable profesiones = new Contexto();
		profesiones.analizarEscenarios();
		profesiones.plantearEscenarios();
		
	}
	
	public static void main(String[] args) {
		new TuPerfil().moderar();
	}

}
